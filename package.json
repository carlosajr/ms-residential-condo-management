{
  "name": "plataforma-microservice",
  "version": "1.0.0",
  "main": "src/main/server.ts",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only --require tsconfig-paths/register src/main/server.ts",
    "start": "node dist/main/server.js",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist",
    "copy:docs": "cp src/shared/http/docs/swagger.json dist/shared/http/docs",
    "build": "yarn clean && yarn generate:swagger && babel src --extensions '.ts' --out-dir dist --ignore '**/*.spec.ts' && yarn copy:docs",
    "generate:swagger": "rm -rf src/shared/http/docs/swagger.json && ts-node --files --require tsconfig-paths/register src/shared/http/docs/swagger-builder.ts",
    "typeorm": "ts-node --require tsconfig-paths/register ./node_modules/typeorm/cli.js --dataSource src/shared/providers/typeorm/config/ormConfig.ts",
    "typeorm:migration:create": "node scripts/create-migration.js",
    "test": "jest",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,json,md}\""
  },
  "keywords": [],
  "license": "MIT",
  "description": "",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.782.0",
    "@opensearch-project/opensearch": "^3.5.1",
    "amqplib": "^0.10.7",
    "aws-sdk": "^2.1692.0",
    "bcryptjs": "^3.0.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^5.1.0",
    "fast-glob": "^3.3.3",
    "fs": "^0.0.1-security",
    "handlebars": "^4.7.8",
    "heic-convert": "^2.1.0",
    "helmet": "^8.1.0",
    "ioredis": "^5.6.0",
    "jsonwebtoken": "^9.0.2",
    "mssql": "^11.0.1",
    "multer": "^1.4.5-lts.2",
    "multer-s3": "^3.0.1",
    "nodemailer": "^6.10.0",
    "path": "^0.12.7",
    "pg": "^8.14.1",
    "pino": "^9.6.0",
    "pino-http": "^10.4.0",
    "pino-pretty": "^13.0.0",
    "redis": "^5.6.0",
    "reflect-metadata": "^0.2.2",
    "sharp": "^0.34.0",
    "swagger-ui-express": "^4.6.3",
    "tsconfig-paths": "^4.2.0",
    "tsyringe": "^4.9.1",
    "typeorm": "^0.3.22"
  },
  "devDependencies": {
    "@babel/cli": "^7.27.0",
    "@babel/core": "^7.26.10",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-decorators": "^7.25.9",
    "@babel/preset-env": "^7.26.9",
    "@babel/preset-typescript": "^7.27.0",
    "@types/amqplib": "^0.10.7",
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.13",
    "@types/heic-convert": "^2.1.0",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/multer": "^1.4.12",
    "@types/multer-s3": "^3.0.3",
    "@types/node": "^22.14.0",
    "@types/nodemailer": "^6.4.17",
    "@types/sharp": "^0.31.1",
    "@types/swagger-ui-express": "^4.1.8",
    "@typescript-eslint/eslint-plugin": "^8.30.1",
    "@typescript-eslint/parser": "^8.30.1",
    "babel-plugin-module-resolver": "^5.0.2",
    "babel-plugin-transform-typescript-metadata": "^0.3.2",
    "eslint": "^9.24.0",
    "eslint-config-prettier": "^10.1.2",
    "eslint-import-resolver-typescript": "^4.3.2",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-prettier": "^5.2.6",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-unused-imports": "^4.1.4",
    "prettier": "^3.5.3",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.30.1"
  }
}
